{
    "parser": "babel-eslint",
    "plugins": [
        "babel",
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
        "rules": {
        "comma-dangle": [
            2,
            "always-multiline"
        ],
        "react/jsx-uses-react": 1
    },
    "env": {
        "browser": true,
        "node": true
    }
}